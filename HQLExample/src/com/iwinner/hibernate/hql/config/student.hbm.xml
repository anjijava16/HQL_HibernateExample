<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.iwinner.hibernate.hql.model.Student" table="Student">
		
		<id name="studentId" column="sid">
			<generator class="assigned"/>
		</id>
		<property name="studentName" column="sname" length="10"/>
		<property name="marks"/>
		<property name="branchName" column="branch" length="10"/>
	
	</class>

</hibernate-mapping>